{{!< default}}

{{!-- The main content area --}}
<main id="site-main" class="site-main outer">
    <div class="inner">
        {{#get "tags" limit="all"}}
            <ul class="tags">
                {{#foreach tags}}
                    <li>
                        <a href="{{url}}">{{name}}</a>
                    </li>
                {{/foreach}}
            </ul>
        {{/get}}
    </div>
</main>
